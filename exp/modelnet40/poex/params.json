{
    "dataset": "modelnet40",
    "dimension": 3,
    "batch_size": 25,
    "set_size": 2048,
    "mask_type": "arb_expand",
    "model": "pc_acset_flow",
    "latent_encoder_hidden": [32,64,128,256],
    "latent_dim": 256,
    "trans_params": {
        "transform": ["L","LR","CP","R","L","LR","CP","R","L","LR","CP","R","L","LR","CP"],
        "dimension": 256,
        "coupling_hids": [256,256]
    },
    "use_peq_embed": 1,
    "set_xformer_hids": [32,64,128,256],
    "acflow_params": {
        "dimension": 3,
        "transform": ["AF","TL","TL","TL","TL","AF"],
        "layer_cfg": ["ML","LR","CP2"],
        "rnncp_units": 256,
        "rnncp_layers": 2,
        "linear_hids": [256,256],
        "linear_rank": -1,
        "affine_hids": [256,256],
        "coupling_hids": [256,256],
        "prior": "autoreg",
        "n_components": 40,
        "prior_units": 256,
        "prior_layers": 2,
        "prior_hids": [256,256]
    },
    "epochs": 1000,
    "optimizer": "adam",
    "lr": 0.0001,
    "decay_steps": 100000,
    "decay_rate": 0.5,
    "clip_gradient": 1,
    "exp_dir": "./exp/modelnet40/poex",
    "summ_freq": 100,
    "eval_metrics": ["sam"]
}