{
    "dataset": "omniglot",
    "image_shape": [32,32,1],
    "mask_type": "impute",
    "batch_size": 20,
    "set_size": 10,
    "model": "im_acset_flow",
    "latent_encoder_hidden": [64,128,256,512],
    "latent_dim": 512,
    "transform": ["L","LR","CP","R","L","LR","CP","R","L","LR","CP","R","L","LR","CP"],
    "dimension": 512,
    "coupling_hids": [128,128],
    "use_peq_embed": 1,
    "img_xformer_hids": [32,64],
    "n_bits": 8,
    "data_constraint": 0.9,
    "residual_blocks": 2,
    "n_scale": 4,
    "base_dim": 32,
    "bottleneck": 1,
    "use_aff": 1,
    "skip": 1,
    "pretrain": 0,
    "epochs": 1000,
    "optimizer": "adam",
    "beta": 1.0,
    "lr": 0.0006,
    "decay_steps": 1000000,
    "decay_rate": 0.5,
    "clip_gradient": 1,
    "exp_dir": "./exp/omniglot/poex",
    "summ_freq": 100
}