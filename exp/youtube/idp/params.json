{
    "dataset": "youtube",
    "mask_type": "random",
    "t_dim": 32,
    "image_shape": [64,64,3],
    "batch_size": 4,
    "set_size": 10,
    "model": "im_acidp",
    "n_bits": 8,
    "data_constraint": 0.9,
    "residual_blocks": 2,
    "n_scale": 5,
    "base_dim": 32,
    "bottleneck": 1,
    "use_aff": 1,
    "skip": 1,
    "epochs": 1000,
    "optimizer": "adam",
    "lr": 0.0006,
    "decay_steps": 50000,
    "decay_rate": 0.5,
    "clip_gradient": 1,
    "exp_dir": "./exp/youtube/idp",
    "summ_freq": 100,
    "eval_metrics": ["sam"]
}